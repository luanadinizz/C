#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
    int n1,n2;
    printf("digite n1");
    scanf("%d",&n1);
    printf("digite n2");
    scanf("%d",&n2);
    if(n1>n2)
       printf("o maior numero e o numero 1");
    else
    printf("o maior numero e o numero 2");

return 0;
}
